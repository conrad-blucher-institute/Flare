{
    "chart_name": "Laguna_Madre",
    "CSPEC_version": "1.0.0",
    "data_requests" : [
      {  "key": "SemaphoreInputs",
         "args": {
            "column_name": "Water Measurement",
            "location": "SBI", //Whatever we name it in the mapping
            "source": "NOAATANDC",
            "series": "dWaterTmp",
            "unit": "seconds",
            "type": "float",
            "interval": 3600,
            "range": [0, -144],
            "verificationOverride": 1
         }
            
        },
        {  "key": "SemaphoreInputs",
         "args": {
            "column_name": "Air Measurement",
            "location": "SBI",
            "source": "NOAATANDC",
            "series": "dAirTemp",
            "unit": "seconds",
            "type": "float",
            "interval": 3600,
            "range": [0, -144],
            "verificationOverride": 1
         }
            
        },
        {  "key": "SemaphoreInputs",
         "args": {
            "column_name": "Air Prediction",
            "location": "lagunamadre", //Whatever we name it in the mapping
            "source": "NDFD_EXP",
            "series": "pAirTemp", 
            "unit": "seconds",
            "type": "float",
            "interval": 3600,
            "range": [0, -144],
            "verificationOverride": 1
         }
            
        }
    ],
    "post_processing" : [
      {
        "key": "ArithmeticOperation",
        "args": {
              "op": "addition",
              "left_col_key": "initial_data",
              "right_col_key": "initial_data",
              "out_col_key": "initial_data_doubled"   
        }
      },
      {
        "key": "ImmediateArithmeticOperation",
        "args": {
            "op": "addition",
            "left_col_key": "initial_data_doubled",
            "value": 7.5,
            "out_col_key": "initial_data_doubled_plus"   
        }
      },
      {
        "key": "LinearInterpolation",
        "args": {
            "col_name": "initial_data_doubled_plus",
            "interpolation_interval": 3600,
            "limit": 5000
        }
      }
    ],
    "csv_config": {
      "csv_name": "Laguna-Madre_Water-Level-Air-Temperature_120hrs.csv",
      "included_columns": [
        "Water Measurement",
        "Water Prediction",
        "Air Measurement",
        "Air Prediction"
      ]
    }
  }
  